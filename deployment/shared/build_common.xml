<?xml version="1.0"?>
<project name="common" default="common">

	<property name="proj_common" value="../../common/net.sf.okapi.common"/> 
	<property name="src_common" value="${proj_common}/src"/> 
 
	<target name="common">
 		<javac srcdir="${src_common}" destdir="${tmp}"/> 
		<copy todir="${tmp}">
			<fileset dir="${src_common}" excludes="**/*.java"/>
		</copy>
	</target>

</project>
